#include <stdio.h>
#include <stdlib.h>
void toh(int n, char s, char d, char t)
{
    if (n > 1)
    {
        toh(n - 1, s, t, d);
        printf("\n move %d disc from %c is %c\n", n, s, d);
        toh(n - 1, t, d, s);
    }
    else
        printf("\n Move %d disc from %c to %c\n", n, s, d);
}
int main()
{
    int n;
    printf("Read value of disc: ");
    scanf("%d", &n);
    toh(n, 'S', 'D', 'T');
    return 0;
}
